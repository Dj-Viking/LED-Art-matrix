  "name": "led-art-matrix",
  "version": "1.9.1",
  "private": true,
  "scripts": {
    "start": "node scripts/start.js",
    "build": "node scripts/build.js",
    "test": "npm run clear:jest; node scripts/test.js --updateSnapshot --verbose --watchAll",
    "test:coverage": "npm run clear:jest; node scripts/test.js --updateSnapshot --verbose --watchAll --coverage",
    "test:ci": "npm run clear:jest; node scripts/test.js --updateSnapshot --verbose --watchAll=false --coverage",
