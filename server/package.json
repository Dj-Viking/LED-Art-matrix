{
  "name": "mern-server",
  "version": "1.2.0",
  "description": "server to the led-matrix application",
  "main": "index.js",
  "scripts": {
    "start": "ENV_TXT=$(cat env.txt) cd dist; node index.js",
    "tsc": "tsc",
    "dev": "ENV_TXT=$(cat env.txt) cd dist; nodemon -L index.js",
    "tsw": "tsc -w",
    "clear:jest": "jest --clearCache",
    "server:ci": "npm run clear:jest; jest --verbose --detectOpenHandles --watchAll=false --coverage",
    "test": "ENV_TXT=$(cat env.txt) jest --verbose --detectOpenHandles --watchAll",
    "test:coverage": "ENV_TXT=$(cat env.txt) jest --verbose --detectOpenHandles --watchAll --coverage",
    "concurrently": "ENV_TXT=$(cat env.txt) concurrently \"npm run tsw\" \"npm run dev\""
  },
  "author": "Anders Ackerman",
  "license": "MIT",
  "dependencies": {
    "@jest/types": "^27.4.2",
    "@typegoose/typegoose": "^9.2.0",
    "@types/bcrypt": "^5.0.0",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/jest": "^27.4.0",
    "@types/jsonwebtoken": "^8.5.6",
    "@types/mongoose-unique-validator": "^1.0.5",
    "@types/node": "17.0.5",
    "@types/node-fetch": "^3.0.3",
    "@types/superagent": "^4.1.13",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^5.4.0",
    "@typescript-eslint/parser": "^5.4.0",
    "babel-cli": "^6.26.0",
    "babel-jest": "^27.3.1",
    "babel-preset-env": "1.7.0",
    "bcrypt": "^5.0.0",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "eslint": "^8.3.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "express": "^4.17.1",
    "jest": "^27.4.7",
    "jsonwebtoken": "^8.5.1",
    "jwt-decode": "^3.1.2",
    "mongoose": "^6.0.13",
    "mongoose-unique-validator": "^3.0.0",
    "node-fetch": "^2.6.1",
    "nodemailer": "^6.6.5",
    "prettier": "^2.5.0",
    "stripe": "^8.137.0",
    "supertest": "^6.2.1",
    "ts-jest": "^27.1.2",
    "ts-node": "^10.4.0",
    "typescript": "^4.5.2",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@types/nodemailer": "^6.4.4",
    "nodemon": "^2.0.7"
  }
}
